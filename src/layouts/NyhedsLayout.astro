---
import MainLayout from "./MainLayout.astro";
const {frontmatter} = Astro.props;
---
<MainLayout title="Titel">
	<div class="max-w-5xl mx-auto px-4 nyhedslayout">	
		<img src={frontmatter.coverimage} alt={frontmatter.coveralt} class="mb-8"/>
		<section class="prose mx-auto">
			<h1 class="text-center" >{frontmatter.title}</h1>
			<slot />
		</section>
	</div>
</MainLayout>